.screen-catalog {
    display: flex;
    justify-content: space-between;
}

.left {
    width: 20%;
    height: 804px;
    background-color: #ffffff;
}

.menu_title {
    font-size: 16px;
    font-family: Microsoft YaHei;
    font-weight: 400;
    color: rgba(1, 4, 15, 1);
    line-height: 48px;
    margin-left: 14px;
    border-bottom: 1px solid rgba(239, 239, 239, 1);
    display: flex;
    justify-content: space-between;
}

.menu_collapse{
  margin-top: 10px;
}
.menu_folder {
  color: #3B3C43;
  padding-left: 25px;
  margin-bottom: 0;
  line-height: 36px;
  cursor: pointer;

}
.menu_folder:hover, .menu_folder.active {
  background: #617BFF;
  line-height: 36px;
  color: #fff; 
  margin: 0 -20px;
  padding-left: 45px;
}

.menu_icon {
    margin-right: 14px;
    margin-top: 16px;
}

.menu_search {
    width: 90%;
    height: 32px;
    margin-left: 14px;
    margin-right: 14px;
    margin-top: 17px;
    font-size: 14px;
}

input::-webkit-input-placeholder {
    font-size: 14px;
    font-family: Microsoft YaHei;
    font-weight: 400;
    color: rgba(210, 210, 213, 1);
}

.right {
    width: 79%;
    height: 48px;
    background: rgba(255, 255, 255, 1);
    float: right;
}

.nav_title {
    font-size: 16px;
    font-family: Microsoft YaHei;
    font-weight: bold;
    color: rgba(1, 4, 15, 1);
    line-height: 48px;
    margin-left: 10px;
}

.btn_pr {
    width: 88px;
    height: 30px;
    float: right;
    margin-top: 9px;
    margin-right: 12px;
}

.btn_n1 {
    width: 88px;
    height: 30px;
    float: right;
    margin-top: 9px;
    margin-right: 18px;
}

.btn_n2 {
    width: 88px;
    height: 30px;
    float: right;
    margin-top: 9px;
    margin-right: 12px;
}

.empty {
    width: 200px;
    height: 259px;
    margin-left: 35%;
    margin-top: 15%;
}

.empty_img {
    width: 225px;
    height: 210px;
}

.empty_word {
    font-size: 12px;
    font-family: Microsoft YaHei;
    font-weight: 400;
    color: rgba(147, 147, 149, 1);
    line-height: 48px;
    text-align: center;
}

.drow_menu {
    width: 132px;
    // height: 84px;
}

.mod_btn {
    width: 310px;
    display: flex;
    justify-content: space-between;
}

.mod_input {
    width: 310px;
}

.mod_icon {
    margin-top: 10px;
}

.mod_span {
    margin-top: 5px;
    font-size: 14px;
    font-family: Microsoft YaHei;
    font-weight: 400;
    color: rgba(1, 3, 15, 1);
}

.add_input {
    height: 28px;
    border: 1px solid rgba(97, 123, 255, 1);
}

.menu-wrap{
  padding: 14px;
}

