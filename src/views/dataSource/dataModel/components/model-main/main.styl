.model-main{
        position: relative;
        height: 100%;
        // overflow-y: auto;
        background-color: #ffffff;
        width: 79%;
        /deep/ .ant-spin-container {
            height calc(100vh - 101px)
            display: flex;
            flex-direction: column;
        }
        .header {
            display: flex;
            justify-content: space-between;
            .data_con {
                overflow: hidden;
                text-overflow: ellipsis;
                white-space: nowrap;
                width: 350px;
                height: 40px;
                font-size: 16px;
                font-family: Microsoft YaHei;
                font-weight: bold;
                color: rgba(1, 4, 15, 1);
                line-height: 48px;
                margin-left: 21px;
            }
            .data_btn {
                width: 188px;
                display: flex;
                justify-content: space-between;
                margin-top: 9px;
                margin-right: 20px;
            }
        }
        .scrollable {
            width 100%
            flex 1
            overflow-y auto    
        }
        .description {
            padding-left: 20px;
            margin-bottom: 20px;
            margin-top: 10px;
            border-bottom: 1px solid #e8e8e8;
            .d-s{
                font-size:14px;
                font-family:Microsoft YaHei;
                font-weight:400;
                color:rgba(59,60,67,1);
                white-space: nowrap;
                text-overflow: ellipsis;
                overflow: hidden;
                max-width: calc(100% - 35px);
                display: inline-block;
                height: 20px;
                line-height: 20px;
            }
        }
        .detail_header {
            margin-left: 21px;
            height: 50px;
            font-size: 16px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: rgba(1, 4, 15, 1);
            // line-height: 48px;
        }
        .detail_main {
            display: flex;
            justify-content: space-around;
            .dimensionality {
                margin-left: 21px;
                width: 50%;
                background: rgba(248, 248, 248, 1);
                .dim_span {
                    font-size:14px;
                    font-family:Microsoft YaHei;
                    font-weight:400;
                    color:rgba(1,4,15,1);
                    line-height:38px;
                    margin-left: 40px;
                }
                .dim_menu {
                    margin-top: 10px;
                    overflow-y: scroll;
                    height: 190px;
                }
            }
            .measurement {
                margin-right: 21px;
                width: 50%;
                background: rgba(248, 248, 248, 1);
                .mea_span {
                    font-size:14px;
                    font-family:Microsoft YaHei;
                    font-weight:400;
                    color:rgba(1,4,15,1);
                    line-height:38px;
                    margin-left: 50px;
                }
                .mea_menu {
                    margin-top: 10px;
                    overflow-y: scroll;
                    height: 190px;
                }
            }
        }
    }

.ant-col-4 {
    text-align: left;
}

.ant-form-explain {
    float: left;
}
.m-dml-map {
    height: calc(50vh - 150px);
    background: #fff;
}
.m-map {
    position: relative;
    box-sizing: border-box;
    margin: 21px;
    overflow: auto;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    -o-user-select: none;
    user-select: none;
    // border: 10px solid #f6f6f6;
}
.m-map-hover {
    border: 2px solid #90b9e8;
}
.main-empty{
    position: absolute;
    margin: 0 auto;
    left: 0;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    color: rgba(0, 0, 0, 0.65);
}