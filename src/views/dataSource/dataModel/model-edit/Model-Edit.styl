        .sheet_list {
            margin-top: 15px;
            font-size: 16px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: #01040f;
            line-height: 1.5;
            overflow-y: auto;
            height: 400px;
            .sheet_list_item {
                font-family: Microsoft YaHei;
                font-weight: 400;
                color: #020510;
                cursor: move;
                cursor: -webkit-grab;
                cursor: -ms-grab;
                cursor: -o-grab;
                overflow: hidden;
                word-wrap: normal;
                white-space: nowrap;
                text-overflow: ellipsis;
                border-left: 2px solid transparent;
                padding: 0 28px 0 22px;
                height: 30px;
                line-height: 30px;
                font-size: 14px;
                &:hover {
                    background: #627CFF;
                    color: #fff;
                }
            }
        }
        .sheet_list::-webkit-scrollbar {
            width: 8px;
            height: 8px;
        }

        .sheet_list::-webkit-scrollbar-thumb {
            background-color: rgba(0, 0, 0, 0.05);
            border-radius: 5px;
            -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1);
        }

        .sheet_list:hover::-webkit-scrollbar-thumb {
            background-color: rgba(0, 0, 0, 0.2);
            border-radius: 5px;
            -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1);
        }

        .sheet_list::-webkit-scrollbar-thumb:hover {
            background-color: rgba(0, 0, 0, 0.4);
            -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1);
        }

        .sheet_list::-webkit-scrollbar-track {
            border-radius: 5px;
            -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0);
            background-color: white;
        }

        .sheet_list::-webkit-scrollbar-track:hover {
            -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.4);
            background-color: rgba(0, 0, 0, 0.01);
        }
        .view-title{
