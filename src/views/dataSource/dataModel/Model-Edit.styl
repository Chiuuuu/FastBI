.Model-Edit {
    display: flex;
    justify-content: space-between;
    .left {
        width: 20%;
        height: 804px;
        background-color: #ffffff;
        .menu_title {
            font-size: 16px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: rgba(1, 4, 15, 1);
            line-height: 48px;
            margin-left: 14px;
            display: flex;
            justify-content: space-between;
        }
        .selector {
            border-left: 3px solid #617BFF;
            margin-left: 14px;
            // height: 64px;
        }
        .menu_search {
            width: 90%;
            height: 32px;
            margin-left: 14px;
            margin-right: 14px;
            margin-top: 17px;
            font-size: 14px;
            .search_span {
                font-size: 16px;
                font-family: Microsoft YaHei;
                font-weight: 400;
                color: rgba(1, 4, 15, 1);
            }
            .search_input {
                margin-top: 17px;
            }
        }
        .sheet_list {
            margin-top: 80px;
            margin-left: 26px;
            font-size: 14px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: rgba(1, 4, 15, 1);
        }
    }
    .right {
        background-color: #ffffff;
        width: 79%;
        .header {
            display: flex;
            justify-content: space-between;
            .data_con {
                width: 201px;
                height: 19px;
                font-size: 16px;
                font-family: Microsoft YaHei;
                font-weight: bold;
                color: rgba(1, 4, 15, 1);
                line-height: 48px;
                margin-left: 21px;
            }
        }
        .description {
            border-bottom: 1px solid #D3D3D3;
            width: 97%;
            height: 35px;
            margin-left: 20px;
            margin-top: 24px;
        }
        .draw_board {
            height: 35%;
            border-bottom: 1px solid #D3D3D3;
            margin-left: 20px;
            width: 97%;
        }
        .detail_header {
            display: flex;
            justify-content: space-between;
            margin-left: 21px;
            height: 50px;
            font-size: 16px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: rgba(1, 4, 15, 1);
            line-height: 48px;
            .detail_btn {
                display: flex;
                justify-content: space-between;
                width: 215px;
                margin-top: 9px;
                margin-right: 20px;
            }
        }
        .detail_main {
            display: flex;
            justify-content: space-around;
            .dimensionality {
                margin-left: 21px;
                height: 32px;
                width: 50%;
                background: rgba(248, 248, 248, 1);
                .dim_span {
                    font-size:14px;
                    font-family:Microsoft YaHei;
                    font-weight:400;
                    color:rgba(1,4,15,1);
                    line-height:38px;
                    margin-left: 40px;

                }
                .dim_menu {
                    margin-top: 10px;
                }
                .dim_operation {
                    float: right;
                }
            }
            .measurement {
                margin-right: 21px;
                height: 32px;
                width: 50%;
                background: rgba(248, 248, 248, 1);
                .mea_span {
                    font-size:14px;
                    font-family:Microsoft YaHei;
                    font-weight:400;
                    color:rgba(1,4,15,1);
                    line-height:38px;
                    margin-left: 50px;
                }
                .mea_menu {
                    margin-top: 10px;
                }
                .mea_operation {
                    float: right;
                }
            }
        }
        .submit_btn {
            float: right;
            margin-right: 44px;
            margin-top: 300px;
            display: flex;
            justify-content: space-between;
            width: 280px;
        }
    }
}

.mod {
    width: 751px;
    height: 310px;
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid rgba(239, 239, 239, 2);
    border-top: 1px solid rgba(239, 239, 239, 2);
    .modal_l {
        width: 50%;
        .set {
            margin-top: 31px;
            margin-left: 25px;
        }
        .modal_select {
            width: 270px;
            display: flex;
            justify-content: space-around;
        }
    }
    .modal_r {
        width: 50%;
        background: rgba(246, 246, 246, 1);
        display: flex;
        justify-content: space-around;
        .bar {
            width: 158px;
            height: 246px;
            // border: 3px solid rgb(255, 252, 252);
            margin-top: 31px;
            margin-left: 40px;
        }
        .descript {
            margin-top: 13px;
            margin-left: 11px;
        }
        .text {
            width: 135px;
            height: 246px;
            margin-top: 32px;
            margin-right: 23px;
            margin-left: 10px;
            font-size: 14px;
            font-family: Microsoft YaHei;
            font-weight: 400;
            color: rgba(1, 4, 15, 1);
        }
    }
    .ant-form-item-label {
        text-align: left;
    }
}