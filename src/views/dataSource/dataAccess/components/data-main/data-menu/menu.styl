.menu {
    height: 470px;
    background-color: #ffffff;
    .menu_title {
        font-size: 16px;
        font-family: Microsoft YaHei;
        font-weight: 400;
        color: rgba(1, 4, 15, 1);
        line-height: 48px;
        margin-left: 14px;
        border-bottom: 1px solid rgba(239, 239, 239, 1);
        display: flex;
        justify-content: space-between;
        .menu_icon {
            margin-right: 14px;
            margin-top: 16px;
        }
    }
    .menu_search {
        width: 90%;
        height: 32px;
        margin-left: 14px;
        margin-right: 14px;
        margin-top: 17px;
        font-size: 14px;
    }
    .menu_tips{
        font-size: 12px;
        color: #999999;
        text-align: left;
        width: 90%;
        margin: 0 auto;
    }
    .table_list-_empty{
        color: rgba(0, 0, 0, 0.65);
    }
}
.card-img{
    height 100px;
}
.card-title {
    color: #333;
    text-align: center;
    text-transform: capitalize;
}