.main {
    min-height: 804px;
    background-color: #ffffff;
    .data_con {
        width: 201px;
        height: 19px;
        font-size: 16px;
        font-family: Microsoft YaHei;
        font-weight: bold;
        color: rgba(1, 4, 15, 1);
        line-height: 48px;
        margin-left: 21px;
    }
    .data_category {
        width: 201px;
        height: 19px;
        font-size: 16px;
        font-family: Microsoft YaHei;
        color: rgba(1, 4, 15, 1);
        line-height: 48px;
    }
    .tab-excel{
        margin-top: 21px;
        margin-left: 21px;
        text-align: left;
        .excel-list{
            border:1px solid rgba(229,229,229,1);
            width: 536px;
            max-height: 300px;
            overflow-y: auto;
        }
        .excel-list-item {
            margin-left 10px
            white-space nowrap
            overflow hidden
            text-overflow ellipsis   
        }
        .preview-list {
            border-top: 1px solid #e5e5e5;
            .preview-tab {
                background-color #f6f6f6;
                padding-top: 5px;
                padding-left: 5px;
                margin-right: 20px;
                .tab-title {
                    color: #999;
                    font-size: 12px;
                    margin-bottom: 10px;
                    margin-top: 20px;
                }
            }
            .preview-tab-item {
                padding-top 5px
                padding-bottom 5px
                cursor pointer
                &:hover, &.active {
                    background-color #fff
                }  
            }
            .preview-controller {
                display flex
                align-items: center
                margin 20px 0
            }
            .preview-line {
                display flex
                margin 0 5px
                .arrow-box {
                    position relative  
                }
                .arrow {
                    position absolute
                    left 0
                    font-size 16px  
                    width 16px
                    height 16px
                    border 1px solid #bbb 
                    cursor pointer 
                    &:nth-child(1) {
                        top 0   
                    }
                    &:nth-child(2) {
                        top 16px    
                    }
                    &:hover {
                        border-color #8aa1ff
                    }
                    i {
                        position absolute
                        left 4px
                        display block
                        width 6px
                        height 6px
                        border 1px solid #bbb
                        border-bottom 0    
                        border-right 0
                        &:hover {
                            border-color #8aa1ff
                        }
                    }
                }
                .arrow-up {
                    top 5px
                    transform: rotate(45deg)
                }
                .arrow-down {
                    top 3px
                    transform: rotate(-135deg)
                }
            }
            .preview-table{
                .ant-table-body::-webkit-scrollbar {
                    /*滚动条整体样式*/
                    width: 10px;
                    /*高宽分别对应横竖滚动条的尺寸*/
                    height: 1px;
                }
                .ant-table-body::-webkit-scrollbar-thumb {
                    /*滚动条里面小方块*/
                    border-radius: 10px;
                    -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
                    background: #ccc;
                }
                .ant-table-body::-webkit-scrollbar-track {
                    /*滚动条里面轨道*/
                    -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
                    // border-radius: 10px;
                    background: #ededed;
                }
            }
        }
    }
    .tab-empty{
        position: absolute;
        margin: 0 auto;
        left: 0;
        right: 0;
        top: 50%;
        transform: translateY(-50%);
        color: rgba(0, 0, 0, 0.65);
    }
    .tab-datasource {
        margin-top: 21px;
        margin-left: 21px;
        text-align: left;
    }
    .tab-datasource-model {
        width: 700px;
    }
    .btn_sub {
        float: right;
        margin-right: 32px;
        margin-top: 13%;
        width: 88px;
        height: 30px;
    }
    .search_bar{
        margin-right: 21px;
    }
    .search_input {
        width: 200%;
        height: 32px;
        margin-left: 20px;
    }
    .search_select {
        width: 300%;
        height: 32px;
        margin-left: 17px;
    }
    .search_button{
        // margin-left: 291px;
        margin-right: 21px;
        }
    .table {
        margin-top: 24px;
        margin-left: 20px;
        width: 98.5%;
        overflow-y: scroll;
        height: 640px
    }
    .table::-webkit-scrollbar {
        width: 8px;
        height: 8px;
    }
    .table::-webkit-scrollbar-thumb {
        background-color: rgba(0, 0, 0, .05);
        border-radius: 5px;
        -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, .1);
    }
    .table:hover::-webkit-scrollbar-thumb {
        background-color: rgba(0, 0, 0, .2);
        border-radius: 5px;
        -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, .1);
    }
    .table::-webkit-scrollbar-thumb:hover {
        background-color: rgba(0, 0, 0, .4);
        -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, .1);
    }
    .table::-webkit-scrollbar-track {
        border-radius: 5px;
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0);
        background-color: white;
    }
    .table::-webkit-scrollbar-track:hover {
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, .4);
        background-color: rgba(0, 0, 0, .01);
    }
    .table a{
        color: #627CFF;
    }
    .bar-all{
        margin-left: 800px;
        }
}

.ant-col-4 {
    text-align: left;
}

.ant-form-explain {
    float: left;
}