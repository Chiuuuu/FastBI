.main {
    min-height: 804px;
    background-color: #ffffff;
    .data_con {
        width: 201px;
        height: 19px;
        font-size: 16px;
        font-family: Microsoft YaHei;
        font-weight: bold;
        color: rgba(1, 4, 15, 1);
        line-height: 48px;
        margin-left: 21px;
    }
    .data_category {
        width: 201px;
        height: 19px;
        font-size: 16px;
        font-family: Microsoft YaHei;
        color: rgba(1, 4, 15, 1);
        line-height: 48px;
    }
    .tab-excel{
        margin-top: 21px;
        margin-left: 21px;
        width: 700px;
        text-align: left;
        .excel-list{
            margin-left: 120px;
            border:1px solid rgba(229,229,229,1);
            width: 530px;
            }
        }
    .tab-empty{
        position: absolute;
        margin: 0 auto;
        left: 0;
        right: 0;
        top: 50%;
        transform: translateY(-50%);
        color: rgba(0, 0, 0, 0.65);
    }
    .tab-datasource {
        margin-top: 21px;
        margin-left: 21px;
        text-align: left;
    }
    .tab-datasource-model {
        width: 700px;
    }
    .btn_sub {
        float: right;
        margin-right: 32px;
        margin-top: 13%;
        width: 88px;
        height: 30px;
    }
    .search_bar{
        margin-right: 21px;
    }
    .search_input {
        width: 200%;
        height: 32px;
        margin-left: 20px;
    }
    .search_select {
        width: 300%;
        height: 32px;
        margin-left: 17px;
    }
    .search_button{
        // margin-left: 291px;
        margin-right: 21px;
        }
    .table {
        margin-top: 24px;
        margin-left: 20px;
        width: 98.5%;
        overflow-y: scroll;
        height: 640px
    }
    .table::-webkit-scrollbar {
        width: 8px;
        height: 8px;
    }
    .table::-webkit-scrollbar-thumb {
        background-color: rgba(0, 0, 0, .05);
        border-radius: 5px;
        -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, .1);
    }
    .table:hover::-webkit-scrollbar-thumb {
        background-color: rgba(0, 0, 0, .2);
        border-radius: 5px;
        -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, .1);
    }
    .table::-webkit-scrollbar-thumb:hover {
        background-color: rgba(0, 0, 0, .4);
        -webkit-box-shadow: inset 1px 1px 0 rgba(0, 0, 0, .1);
    }
    .table::-webkit-scrollbar-track {
        border-radius: 5px;
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0);
        background-color: white;
    }
    .table::-webkit-scrollbar-track:hover {
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, .4);
        background-color: rgba(0, 0, 0, .01);
    }
    .table a{
        color: #627CFF;
    }
    .bar-all{
        margin-left: 800px;
        }
}

.ant-col-4 {
    text-align: left;
}

.ant-form-explain {
    float: left;
}