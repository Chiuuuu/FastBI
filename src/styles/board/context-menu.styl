@import "../common/var.styl"
.dv-context-menu {
  position: fixed;
  z-index: 9999;
  background: $color-admin-context-menu;
  color: $color-admin-context-menu-color;
  user-select: none;
  .context-menu-item {
    padding: 0 6px;
    line-height: 28px;
    height: 28px;
    border-left: 2px solid transparent;
    cursor: pointer;
    // overflow: hidden;
    padding-right: 3em;
    position:relative;
    color: $color-admin-context-menu-color;
    .context-menu-item-children{
      background: $color-admin-context-menu;
      position:absolute;
      right: -80px;
      top: 0;
    }
    .iconfont {
      margin-right: 3px;
      line-height: 28px;
      width: 16px;
    }
    &:hover {
      background-color: #1d262e;
      color: #00baff;
      border-left: 2px solid #00baff;
    }
  }
}
  .chartdata-table{
      table-layout:fixed;
        border-collapse: collapse;
        border-spacing: 0px;
        overflow:scroll;
        border-left:1px solid black;
        border-top:1px solid black;
        .table-td{
            padding:10px;
            text-align:center;
            border-right:1px solid black;
            border-bottom:1px solid black;
        }
  }
